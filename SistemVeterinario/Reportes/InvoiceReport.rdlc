  <Code>
  Public Function NumeroATexto(ByVal numero As Decimal) As String
    Try
      Dim parteEntera As Integer = Math.Floor(numero)
      Dim parteDecimal As Integer = Math.Round((numero - parteEntera) * 100)
      
      Dim textoEntero As String = ConvertirEntero(parteEntera)
      
      Return "Son " + textoEntero + " " + parteDecimal.ToString("00") + "/100 Bolivianos"
    Catch ex As Exception
      Return "Error en conversión"
    End Try
  End Function
  
  Private Function ConvertirEntero(ByVal numero As Integer) As String
    If numero = 0 Then Return "cero"
    
    Dim unidades() As String = {"", "uno", "dos", "tres", "cuatro", "cinco", "seis", "siete", "ocho", "nueve"}
    Dim decenas() As String = {"", "", "veinte", "treinta", "cuarenta", "cincuenta", "sesenta", "setenta", "ochenta", "noventa"}
    Dim especiales() As String = {"diez", "once", "doce", "trece", "catorce", "quince", "dieciséis", "diecisiete", "dieciocho", "diecinueve"}
    Dim centenas() As String = {"", "ciento", "doscientos", "trescientos", "cuatrocientos", "quinientos", "seiscientos", "setecientos", "ochocientos", "novecientos"}
    
    Dim resultado As String = ""
    
    ' Millones
    If numero &gt;= 1000000 Then
      Dim millones As Integer = numero \ 1000000
      If millones = 1 Then
        resultado += "un millón "
      Else
        resultado += ConvertirCentenas(millones, unidades, decenas, especiales, centenas) + " millones "
      End If
      numero = numero Mod 1000000
    End If
    
    ' Miles
    If numero &gt;= 1000 Then
      Dim miles As Integer = numero \ 1000
      If miles = 1 Then
        resultado += "mil "
      Else
        resultado += ConvertirCentenas(miles, unidades, decenas, especiales, centenas) + " mil "
      End If
      numero = numero Mod 1000
    End If
    
    ' Centenas, decenas y unidades
    If numero &gt; 0 Then
      resultado += ConvertirCentenas(numero, unidades, decenas, especiales, centenas)
    End If
    
    Return resultado.Trim()
  End Function
  
  Private Function ConvertirCentenas(ByVal numero As Integer, ByVal unidades() As String, ByVal decenas() As String, ByVal especiales() As String, ByVal centenas() As String) As String
    Dim resultado As String = ""
    
    ' Centenas
    If numero &gt;= 100 Then
      Dim c As Integer = numero \ 100
      If numero = 100 Then
        resultado += "cien"
      Else
        resultado += centenas(c) + " "
      End If
      numero = numero Mod 100
    End If
    
    ' Decenas y unidades
    If numero &gt;= 20 Then
      Dim d As Integer = numero \ 10
      Dim u As Integer = numero Mod 10
      resultado += decenas(d)
      If u &gt; 0 Then
        resultado += " y " + unidades(u)
      End If
    ElseIf numero &gt;= 10 Then
      resultado += especiales(numero - 10)
    ElseIf numero &gt; 0 Then
      resultado += unidades(numero)
    End If
    
    Return resultado.Trim()
  End Function
  </Code>
